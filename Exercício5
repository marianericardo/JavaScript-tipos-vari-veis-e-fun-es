let num1 = 10; 
let num2 = 25; 
let num3 = 15; 

console.log("Números:");
console.log("Número 1:", num1);
console.log("Número 2:", num2);
console.log("Número 3:", num3);

let maior = num1; 

if (num2 > maior) {
    maior = num2; 
}

if (num3 > maior) {
    maior = num3;
}
console.log("O maior número é:", maior);

